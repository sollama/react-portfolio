this.setState({text1: event.target.value})}\r\n\r\n                            ></FormControl>\r\n                    </FormGroup>\r\n                </Form>\r\n                {\r\n                    this.props.memes.slice(0, this.state.memeLimit).map((meme, index) => {\r\n                        return (\r\n                            <MemeItem \r\n                            key={index}\r\n                            meme={meme}\r\n                            text0={this.state.text0}\r\n                            text1={this.state.text1}\r\n                            />\r\n                        )\r\n                    })\r\n                }\r\n                <div className=\"meme-button\" onClick={() => {\r\n                    this.setState({memeLimit: this.state.memeLimit+10})\r\n                }}>Load 10 more memes...</div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nfunction mapStateToProps(state) {\r\n    return state;\r\n}\r\n\r\nexport default connect(mapStateToProps, null)(MemeGallery);"},"lineCount":null}},"error":null,"hash":"eab97459263deff8565ea0bb34acf964","cacheData":{"env":{}}}